  "main": "src/index.js",
  "scripts": {
    "dev:maputnik": "maputnik --watch -f src/style/style.json",
    "dev:app": "webpack-dev-server --config webpack.config.js --progress --hot",
    "develop": "npm-run-all -p dev:app dev:maputnik",
    "preprocess": "npm-run-all pre:osm pre:goog pre:vtile",
    "pre:vtile": "cd src/scripts && bash ./gen-vector-tiles.sh",
    "pre:osm": "cd src/scripts && node ./get-osm-data.js",
    "pre:goog": "cd src/scripts && node ./extract-google-pois.js ../data/raw/saved-places.json ../data/pyr/pois.geojson",
    "build:vtile": "mbtiles2ungzpbf -f public/data/pyr.mbtiles -d public/data/pyr/",
    "build:app": "webpack --mode=production",
    "build": "npm-run-all build:vtile build:app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
